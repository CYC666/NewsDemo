#import <Foundation/Foundation.h>

@interface HMYGGoodsModel : NSObject


@property (copy, nonatomic) NSString *ProductId;                //商品ID
@property (copy, nonatomic) NSString *ProductName;              //商品名
@property (copy, nonatomic) NSString *Price;                    //价格
@property (copy, nonatomic) NSString *SortCode;                 //排序码
@property (copy, nonatomic) NSString *CreateDate;               //上架时间
@property (copy, nonatomic) NSString *FilePath;                 //图片
@property (copy, nonatomic) NSString *Counts;                   //购买数量
@property (copy, nonatomic) NSString *Description;              //商品描述

- (instancetype)initWithProductId:(NSString *)ProductId
                      ProductName:(NSString *)ProductName
                            Price:(NSNumber *)Price
                         SortCode:(NSNumber *)SortCode
                       CreateDate:(NSString *)CreateDate
                         FilePath:(NSString *)FilePath
                           Counts:(NSNumber *)Counts
                      Description:(NSString *)Description;


@end
