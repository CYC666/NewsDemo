#import <Foundation/Foundation.h>

@interface SurrenderMainInfo : NSObject

//主键编号
@property (nonatomic) NSString *businesspaymentid;
//userid
@property (nonatomic) NSString *userid;
//让利金额
@property (nonatomic) NSString *operatevalue;
//
@property (nonatomic) NSString *operateovervalue;
//
@property (nonatomic) NSString *getintegral;
//状态值
@property (nonatomic) NSString *paymentmark;
//
@property (nonatomic) NSString *singlesystemmark;
//创建时间
@property (nonatomic) NSString *createdate;
//描述
@property (nonatomic) NSString *Description;
//审计描述
@property (nonatomic) NSString *auditdescription;
//修改时间
@property (nonatomic) NSString *modifydate;
//修改人ID
@property (nonatomic) NSString *modifyuserid;
