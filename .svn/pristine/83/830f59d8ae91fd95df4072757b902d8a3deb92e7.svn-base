@property (nonatomic) NSString *modifyusername;


@property (nonatomic) NSString *itemname;
@property (nonatomic) NSString *aescriptions;

///初始化
- (instancetype)initWithbusinesspaymentid:(NSString *)businesspaymentid
                                   userid:(NSString *)userid
                             operatevalue:(NSString *)operatevalue
                         operateovervalue:(NSString *)operateovervalue
                              getintegral:(NSString *)getintegral
                              paymentmark:(NSString *)paymentmark
                         singlesystemmark:(NSString *)singlesystemmark
                               createdate:(NSString *)createdate
                              Description:(NSString *)Description
                         auditdescription:(NSString *)auditdescription
                               modifydate:(NSString *)modifydate
                             modifyuserid:(NSString *)modifyuserid
                           modifyusername:(NSString *)modifyusername;



///初始化
- (instancetype)initWithitemname:(NSString *)itemname descriptions:(NSString *)descriptions;
@end
