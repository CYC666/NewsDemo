#import "CSearchView.h"

@implementation CSearchView

- (void)awakeFromNib {

    [super awakeFromNib];
    
    self.searchField.tintColor = [UIColor grayColor];                                       // 光标颜色
    self.searchField.contentVerticalAlignment = UIControlContentVerticalAlignmentCenter;    // 垂直方向的对齐方式

}

@end
